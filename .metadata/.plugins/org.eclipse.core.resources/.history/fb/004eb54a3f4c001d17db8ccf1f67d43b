import java.util.Scanner;
public class SequênciaLógica2 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int X, Y, I;
		X= sc.nextInt();
	    Y= sc.nextInt();
	    for(I=1; I<=Y; I++) {
	    	System.out.print(I+ " ");
	    	if (I%X==0) {
	    		System.out.println();
	    	}
	    }
	}
}