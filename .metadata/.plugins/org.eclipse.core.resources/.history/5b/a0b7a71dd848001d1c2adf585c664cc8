import java.util.Scanner;

public class Intervalo2 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int N, X, i, in=0, out=0;
		N = sc.nextInt();
		X = sc.nextInt();
		for (i = 0; i < N; i++) {
			if (X >= 10 && X <= 20) {
				in++;
			} else {
				out++;
			}
		}
		System.out.println(in + " in");
		System.out.println(out + " out");

	}
}