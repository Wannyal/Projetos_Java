import java.util.Scanner;
public class SequênciaLógica2 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int X, Y, I;
		X= sc.nextInt();
		Y= sc.nextInt();
		for (I=X; I<=Y; I++) {
			System.out.print(I+" ");
		System.out.print(I + 1 + " ");
		System.out.println(I +1 +1);
		
		}
}
}