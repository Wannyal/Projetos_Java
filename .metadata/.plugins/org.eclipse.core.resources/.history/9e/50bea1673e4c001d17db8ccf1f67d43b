import java.util.Scanner;

public class SequênciaLógica2 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int X, Y, I, L;
		X = sc.nextInt();
		Y = sc.nextInt();
		for (I = 1; I <= Y; I++) {
			for(L=0;L<X;L++) {
				System.out.print(L + 1 + " ");
			}
		
			
		}
	}
}