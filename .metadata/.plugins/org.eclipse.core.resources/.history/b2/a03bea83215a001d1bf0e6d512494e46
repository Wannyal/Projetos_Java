import java.util.Scanner;

public class Quest5 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int L, C, I, J;
		L=sc.nextInt();
		C=sc.nextInt();
		for(I=0; I<L; I++) {
			if(L==0 || L==L-1) {
				for(J=0; J<C; J++) {
					System.out.print("*");
				}
			}
				
			System.out.println();
		}

	}
}
