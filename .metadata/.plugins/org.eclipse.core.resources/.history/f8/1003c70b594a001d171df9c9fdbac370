import java.util.Scanner;

public class PositivoseMÃ©dia {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		float N = 0, M = 0, MT, I;
		for (I = 0; I < 6; I++) {
			N = sc.nextFloat();
			if (N > 0) {
				M = M + N;
			}
		}
		MT = M / I;
		System.out.println(I + " valores posiTtivos");
		System.out.println(MT);
	}
}
